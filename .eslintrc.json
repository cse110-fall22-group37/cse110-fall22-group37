{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "never"
        ],
        "no-undef": [
            "off"
        ],
        "no-unused-vars": [
            "off"
        ],
        "no-redeclare": [
            "warn"
        ],
        "no-cond-assign": [
            "warn"
        ],
        "no-useless-escape": [
            "warn"
        ],
        "no-prototype-builtins": [
            "warn"
        ]
    }
}
